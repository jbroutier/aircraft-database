{
    "name": "jbroutier/aircraft-database",
    "description": "Global database of aircraft manufacturers, types, models & engines.",
    "version": "1.10.1",
    "type": "project",
    "license": "MIT",
    "homepage": "https://aircraft-database.com",
    "authors": [
        {
            "name": "Jérémie BROUTIER",
            "email": "jeremie.broutier@posteo.net",
            "role": "developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1.0",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-sysvsem": "*",
        "ext-uuid": "*",
        "babdev/pagerfanta-bundle": "^3.7.0",
        "beberlei/doctrineextensions": "^1.3.0",
        "composer/spdx-licenses": "^1.5.7",
        "doctrine/doctrine-bundle": "^2.6.3",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.12.2",
        "league/commonmark": "^2.3.2",
        "liip/imagine-bundle": "^2.8.0",
        "monolog/monolog": "^2.6.0",
        "mhujer/breadcrumbs-bundle": "^1.5.8",
        "pagerfanta/pagerfanta": "^3.6.1",
        "presta/sitemap-bundle": "^3.3.0",
        "sentry/sentry-symfony": "^4.3.0",
        "symfony/amqp-messenger": "^6.1.0",
        "symfony/asset": "^6.1.0",
        "symfony/console": "^6.1.0",
        "symfony/doctrine-messenger": "^6.1.0",
        "symfony/dotenv": "^6.1.0",
        "symfony/expression-language": "^6.1.0",
        "symfony/flex": "^2.2.1",
        "symfony/form": "^6.1.0",
        "symfony/framework-bundle": "^6.1.0",
        "symfony/intl": "^6.1.0",
        "symfony/mailer": "^6.1.0",
        "symfony/messenger": "^6.1.0",
        "symfony/monolog-bundle": "^3.8.0",
        "symfony/rate-limiter": "^6.1.0",
        "symfony/runtime": "^6.1.0",
        "symfony/security-bundle": "^6.1.0",
        "symfony/translation": "^6.1.0",
        "symfony/twig-bundle": "^6.1.0",
        "symfony/uid": "^6.1.0",
        "symfony/validator": "^6.1.0",
        "symfony/web-link": "^6.1.0",
        "symfony/webpack-encore-bundle": "^1.14.1",
        "symfony/yaml": "^6.1.0",
        "twig/extra-bundle": "^3.4.0",
        "twig/intl-extra": "^3.4.0",
        "twig/markdown-extra": "^3.4.0",
        "twig/twig": "^3.4.1",
        "vich/uploader-bundle": "^1.19.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.1.0",
        "davidbadura/faker-markdown-generator": "^1.1.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4.2",
        "ekino/phpstan-banned-code": "^1.0.0",
        "fakerphp/faker": "^1.19.0",
        "mockery/mockery": "^1.5.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.7.11",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-doctrine": "^1.3.9",
        "phpstan/phpstan-mockery": "^1.1.0",
        "phpstan/phpstan-phpunit": "^1.1.1",
        "phpstan/phpstan-strict-rules": "^1.2.3",
        "phpstan/phpstan-symfony": "^1.2.2",
        "phpunit/phpunit": "^9.5.20",
        "symfony/browser-kit": "^6.1.0",
        "symfony/css-selector": "^6.1.0",
        "symfony/maker-bundle": "^1.43.0",
        "symfony/phpunit-bridge": "^6.1.0",
        "symfony/web-profiler-bundle": "^6.1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-uuid": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "phpstan": "vendor/bin/phpstan analyse --memory-limit 1G",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "tests:functional": "bin/phpunit --testsuite=functional --testdox --coverage-clover coverage-functional.xml",
        "tests:unit": "bin/phpunit --testsuite=unit --testdox --coverage-clover coverage-unit.xml"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
